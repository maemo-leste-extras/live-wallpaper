install-accelthemeDATA: $(acceltheme_DATA)
	@$(NORMAL_INSTALL)
	@list='$(acceltheme_DATA)'; test -n "$(accelthemedir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(accelthemedir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(accelthemedir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(accelthemedir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(accelthemedir)" || exit $$?; \
	done

uninstall-accelthemeDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(acceltheme_DATA)'; test -n "$(accelthemedir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(accelthemedir)'; $(am__uninstall_files_from_dir)
